{
  "name": "kumelen-api",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "prisma:init": "prisma init",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.947.0",
    "@aws-sdk/s3-request-presigner": "^3.947.0",
    "@hono/node-server": "^1.19.6",
    "@hono/zod-validator": "^0.4.3",
    "@prisma/client": "^6.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.4.7",
    "hono": "^4.10.7",
    "jose": "^6.1.3",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prisma": "^6.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.15"
  }
}
